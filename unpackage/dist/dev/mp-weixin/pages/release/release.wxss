
page {
  background: #efeff4;
}
.container {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.tabbar {
  font-size: 28rpx;
  color: #909090;
  background: #FFF;
  margin-bottom: 10px;
  padding: 10px;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
  flex-wrap: wrap;
}
.tabbar-item {
  width: 20%;
  line-height: 1.8em;
  text-align: center;
}
.tabbar-item.on {
  color: #FFF;
  background: #fd7d49;
  border-radius: 28rpx;
}
.uni-list {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.uni-media-list-logo {
  width: 180rpx;
  height: 140rpx;
}
.uni-media-list-body {
  height: auto;
  -webkit-justify-content: space-around;
  justify-content: space-around;
}
.uni-media-list-text-top {
  height: 74rpx;
  font-size: 28rpx;
  overflow: hidden;
}
.uni-media-list-text-bottom {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
  flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
  justify-content: space-between;
}
.uni-list:before,
.uni-list:after,
.uni-list-cell:after {
  height: 0rpx;
}
.none {
  color: #909090;
  font-size: 28rpx;
  text-align: center;
  line-height: 3em;
}