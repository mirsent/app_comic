          <view class="_view data-v-649c6790 product-title uni-ellipsis"> {{product.title}} </view>
          <view class="_view data-v-649c6790 product-brief uni-ellipsis"> {{product.brief}} </view>
        </view>
      </view>
    </view>
  </view>
</template>