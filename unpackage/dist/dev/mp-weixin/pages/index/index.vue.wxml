<template name="240402f2">
  <view class="_view data-v-df821236 container">
    <swiper indicator-dots="{{indicatorDots}}" indicator-active-color="{{indicatorColor}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" class="_swiper data-v-df821236">
      <swiper-item wx:key="index" key="{{index}}" wx:for="{{bannerData}}" wx:for-index="index" wx:for-item="banner" class="_swiper-item data-v-df821236">
        <view class="_view data-v-df821236 swiper-item">
          <image src="{{banner.banner_url}}" class="_image data-v-df821236"></image>
        </view>
      </swiper-item>
    </swiper>
    <view class="_view data-v-df821236 nav">
      <view class="_view data-v-df821236 nav-item" wx:key="index" key="{{index}}" wx:for="{{navData}}" wx:for-index="index" wx:for-item="nav">
        <image src="{{nav.img}}" class="_image data-v-df821236 img"></image>
        <text class="_text data-v-df821236 title">{{nav.title}}</text>
      </view>
    </view>
    <view class="_view data-v-df821236 line">
      <text class="_text data-v-df821236"></text>
    </view>
    <view class="_view data-v-df821236 list" wx:key="i" key="{{i}}" wx:for="{{productData}}" wx:for-index="i" wx:for-item="type">
      <view class="_view data-v-df821236 list-header"> {{type.release_type_name}}
        <view class="_view data-v-df821236 more" bindtap="handleProxy" data-eventid="{{'Ejb-0-'+i}}" data-comkey="{{$k}}">
          <image src="../../static/image/right.png" class="_image data-v-df821236 more-icon"></image>
        </view>
      </view>
      <view class="_view data-v-df821236 list-body">
        <view class="_view data-v-df821236 product" wx:key="index" key="{{index}}" bindtap="handleProxy" data-eventid="{{'vek-1-'+i+'-'+index}}" data-comkey="{{$k}}" wx:for="{{type.products}}" wx:for-index="index" wx:for-item="product">
          <image src="{{product.cover}}" mode="scaleToFill" class="_image data-v-df821236 product-img"></image>
          <view class="_view data-v-df821236 product-title uni-ellipsis"> {{product.title}} </view>
          <view class="_view data-v-df821236 product-brief uni-ellipsis"> {{product.brief}} </view>
        </view>
      </view>
    </view>
  </view>
</template>