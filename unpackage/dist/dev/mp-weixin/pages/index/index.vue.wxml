        <view class="_view data-v-649c6790 more" bindtap="handleProxy" data-eventid="{{'Miw-0-'+i}}" data-comkey="{{$k}}">
          <image src="../../static/image/right.png" class="_image data-v-649c6790 more-icon"></image>
