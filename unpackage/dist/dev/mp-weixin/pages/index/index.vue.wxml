      <view class="_view data-v-649c6790 nav-item" wx:key="index" key="{{index}}" wx:for="{{navData}}" wx:for-index="index" wx:for-item="nav">
        <image src="{{nav.img}}" class="_image data-v-649c6790 img"></image>
        <text class="_text data-v-649c6790 title">{{nav.title}}</text>
      </view>
    </view>
    <view class="_view data-v-649c6790 line">
      <text class="_text data-v-649c6790"></text>
    </view>
    <view class="_view data-v-649c6790 list" wx:key="i" key="{{i}}" wx:for="{{productData}}" wx:for-index="i" wx:for-item="type">
