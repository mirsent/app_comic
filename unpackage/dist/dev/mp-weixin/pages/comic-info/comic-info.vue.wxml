<template name="05675686">
  <view class="_view data-v-47c537ff">
    <view class="_view data-v-47c537ff banner">
      <image class="_image data-v-47c537ff banner-img" src="{{comic.cover}}" mode="scaleToFill"></image>
      <view class="_view data-v-47c537ff banner-title">{{comic.title}}</view>
      <view class="_view data-v-47c537ff banner-info">
        <view class="_view data-v-47c537ff banner-type">
          <view class="_view data-v-47c537ff type-item" wx:key="index" key="{{index}}" wx:for="{{comic.types}}" wx:for-index="index" wx:for-item="type">{{type}}</view>
        </view>
        <view class="_view data-v-47c537ff banner-status">{{comic.s_serial_name}}/更新至{{comic.total_chapter}}话</view>
      </view>
    </view>
    <view class="_view data-v-47c537ff func">
      <view class="_view data-v-47c537ff func-item">
        <view class="_view data-v-47c537ff label" bindtap="handleProxy" data-eventid="{{'qY8-0'}}" data-comkey="{{$k}}">
          <image src="../../static/image/collect.png" class="_image data-v-47c537ff icon"></image>
          <view class="_view data-v-47c537ff title">收藏</view>
        </view>
        <view class="_view data-v-47c537ff label" bindtap="handleProxy" data-eventid="{{'fcE-1'}}" data-comkey="{{$k}}">
          <image src="../../static/image/like.png" class="_image data-v-47c537ff icon"></image>
          <view class="_view data-v-47c537ff title">点赞</view>
        </view>
      </view>
      <view class="_view data-v-47c537ff func-item">
        <view class="_view data-v-47c537ff btn" bindtap="handleProxy" data-eventid="{{'x4q-2'}}" data-comkey="{{$k}}">
          <image src="../../static/image/book.png" class="_image data-v-47c537ff icon"></image>
          <view class="_view data-v-47c537ff title">开始阅读</view>
        </view>
      </view>
    </view>
    <view class="_view data-v-47c537ff tabbar">
      <view class="_view data-v-47c537ff tabbar-item {{[isDetail ? 'on' : '']}}" bindtap="handleProxy" data-eventid="{{'bev-3'}}" data-comkey="{{$k}}">详情</view>
      <view class="_view data-v-47c537ff tabbar-item {{[isChapter ? 'on' : '']}}" bindtap="handleProxy" data-eventid="{{'qBt-4'}}" data-comkey="{{$k}}">选集</view>
    </view>
    <view class="_view data-v-47c537ff body" wx:if="{{isDetail}}">
      <view class="_view data-v-47c537ff info">
        <text class="_text data-v-47c537ff">简介：</text> {{comic.brief}} </view>
      <view class="_view data-v-47c537ff status">
        <view class="_view data-v-47c537ff status-item">
          <image src="../../static/image/see_on.png" class="_image data-v-47c537ff icon icon-see"></image>
          <view class="_view data-v-47c537ff title">总浏览{{comic.popularity}}</view>
        </view>
        <view class="_view data-v-47c537ff status-item">
          <image src="../../static/image/like_on.png" class="_image data-v-47c537ff icon"></image>
          <view class="_view data-v-47c537ff title">{{comic.heat}}人点赞</view>
        </view>
      </view>
    </view>
    <view class="_view data-v-47c537ff body" wx:if="{{isChapter}}">
      <view class="_view data-v-47c537ff uni-list">
        <view class="_view data-v-47c537ff uni-list-cell" hover-class="uni-list-cell-hover" wx:key="index" key="{{index}}" wx:for="{{chapterData}}" wx:for-index="index" wx:for-item="item">
          <view class="_view data-v-47c537ff uni-media-list">
            <image class="_image data-v-47c537ff uni-media-list-logo" src="{{item.chapter_cover}}" wx:if="{{item.chapter_cover}}" mode="aspectFill"></image>
            <image class="_image data-v-47c537ff uni-media-list-logo" src="../../static/image/place.png" mode="scaleToFill" wx:else></image>
            <view class="_view data-v-47c537ff uni-media-list-body">
              <view class="_view data-v-47c537ff uni-media-list-text-top">{{item.catalog_name}} {{item.chapter_title}}</view>
              <view class="_view data-v-47c537ff uni-media-list-text-bottom uni-ellipsis">
                <view class="_view data-v-47c537ff">{{item.create_at}}</view>
                <view class="_view data-v-47c537ff popular">
                  <image src="../../static/image/see.png" class="_image data-v-47c537ff"></image>
                  <view class="_view data-v-47c537ff">{{item.popularity}}</view>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
      <view class="_view data-v-47c537ff go-top" hidden="{{!(isToUp)}}" bindtap="handleProxy" data-eventid="{{'Bk2-5'}}" data-comkey="{{$k}}">
        <image src="../../static/image/top.png" class="_image data-v-47c537ff"></image>
      </view>
    </view>
  </view>
</template>