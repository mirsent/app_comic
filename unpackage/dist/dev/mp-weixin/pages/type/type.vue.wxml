<template name="1a37f6c6">
  <view class="_view data-v-5302ab7f container">
    <view class="_view data-v-5302ab7f tabbar">
      <view class="_view data-v-5302ab7f tabbar-item {{[tabbarIndex == index ? 'on' : '']}}" wx:key="index" key="{{index}}" data-id="{{tabbar.id}}" data-current="{{index}}" bindtap="handleProxy" data-eventid="{{'4TI-0-'+index}}" data-comkey="{{$k}}" wx:for="{{tabbarList}}"
        wx:for-index="index" wx:for-item="tabbar"> {{tabbar.comic_type_name}} </view>
    </view>
    <view class="_view data-v-5302ab7f uni-list">
      <view class="_view data-v-5302ab7f uni-list-cell" wx:key="key" key="{{key}}" bindtap="handleProxy" data-eventid="{{'8OY-1-'+key}}" data-comkey="{{$k}}" wx:for="{{productList}}" wx:for-index="key" wx:for-item="product">
        <view class="_view data-v-5302ab7f uni-media-list">
          <image class="_image data-v-5302ab7f uni-media-list-logo" src="{{product.cover}}" mode="aspectFill"></image>
          <view class="_view data-v-5302ab7f uni-media-list-body">
            <view class="_view data-v-5302ab7f uni-media-list-text-top">{{product.title}}</view>
            <view class="_view data-v-5302ab7f uni-media-list-text-bottom">
              <text class="_text data-v-5302ab7f">{{product.release_type_name}}</text>
              <text class="_text data-v-5302ab7f">{{product.created_at}}</text>
            </view>
          </view>
        </view>
      </view>
      <view class="_view data-v-5302ab7f none" wx:if="{{productList.length == 0}}"> 上新中... </view>
    </view>
  </view>
</template>